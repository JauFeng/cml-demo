<view class=" cml-base cml-view cml-3999c8ed"><c-popup show="{{show}}" onClose="_cmlEventProxy" data-eventclose="{{['cancelclick']}}" position="bottom" class=" cml-base cml-c-popup cml-3999c8ed">
  <view class="action-wrap  cml-base cml-view cml-3999c8ed">
    <view class="action-header  cml-base cml-view cml-3999c8ed" a:if="{{title || pickerStyle}}">
      <text class="action-header-text  cml-base cml-text cml-3999c8ed" style="{{actionHeaderStyle}}">{{title}}</text>
      <view class="cancel-wrap  cml-base cml-view cml-3999c8ed" a:if="{{pickerStyle}}" onTap="_cmlEventProxy" data-eventtap="{{['cancelclick']}}">
        <text class="cancel-text  cml-base cml-text cml-3999c8ed" style="{{actionCancelStyle}}">{{cancelTxt}}</text>
      </view>
    </view>
    <view class="action-content  cml-base cml-view cml-3999c8ed" style="{{actionContentStyle}}">
      <view class="action-list  cml-base cml-view cml-3999c8ed"><cml-buildin-list class="action-list  cml-base cml-list cml-3999c8ed" height="{{listHeight}}">
        <view a:for="{{list}}" a:for-index="i" a:for-item="item" data-idx="{{i}}" data-value="{{item}}" onTap="_cmlEventProxy" data-eventtap="{{['itemClick']}}" class="action-list-item  cml-base cml-cell cml-3999c8ed">
          <text a:if="{{i === active}}" style="{{activeTextStyle}}" class="text-active  cml-base cml-text cml-3999c8ed">{{item}}</text>
          <text a:else style="{{textStyle}}" class="text  cml-base cml-text cml-3999c8ed">{{item}}</text>
        </view>     
      </cml-buildin-list></view>
    </view>
    <block a:if="{{!pickerStyle}}" class=" cml-base cml-block cml-3999c8ed">
      <view class="action-space  cml-base cml-view cml-3999c8ed"></view>
      <text style="{{actionCancelStyle}}" onTap="_cmlEventProxy" data-eventtap="{{['cancelclick']}}" class="cancel-btn  cml-base cml-text cml-3999c8ed">{{cancelTxt}}</text>
    </block>
  </view>
  </c-popup></view>