.cml-cf9f3b62.cml-base {  
  text-align: left;
  /* letter-spacing: 1rpx; */
  font-family: BlinkMacSystemFont, 'Source Sans Pro', 'Helvetica Neue', Helvetica, Arial, sans-serif;
  font-size: 32rpx;
}

.cml-cf9f3b62.cml-base,
.cml-cf9f3b62.cml-base::before,
.cml-cf9f3b62.cml-base::after {
  box-sizing: border-box;
  -webkit-text-size-adjust: none;
     -moz-text-size-adjust: none;
      -ms-text-size-adjust: none;
          text-size-adjust: none;
}

.cml-cf9f3b62.cml-view {
  box-sizing: border-box;
  border: 0 solid black;
  margin: 0;
  padding: 0;
  
  position: relative;

  display: -webkit-box;

  display: -webkit-flex;

  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;

  -webkit-box-flex: 0;

  -webkit-flex: 0 0 auto;

          flex: 0 0 auto;
}

.cml-cf9f3b62.cml-text {
  box-sizing: border-box;
  border: 0 solid black;
  margin: 0;
  padding: 0;
  
  position: relative;
  
  display: block;
  
  -webkit-box-flex: 0;
  
  -webkit-flex: 0 0 auto;
  
          flex: 0 0 auto;

  white-space: pre-wrap; /* not using 'pre': support auto line feed. 保留空白符序列，但是正常地进行换行 */
  word-wrap: break-word; /* 在长单词或 URL 地址内部进行换行。 */
  overflow: hidden; /* it'll be clipped if the height is not high enough. */
}

.cml-cf9f3b62.cml-border-1px,
.cml-cf9f3b62.cml-border-top-1px,
.cml-cf9f3b62.cml-border-right-1px,
.cml-cf9f3b62.cml-border-bottom-1px,
.cml-cf9f3b62.cml-border-left-1px {
  position: relative;
}

.cml-cf9f3b62.cml-border-top-1px:before,
.cml-cf9f3b62.cml-border-left-1px:before,
.cml-cf9f3b62.cml-border-1px:after,
.cml-cf9f3b62.cml-border-right-1px:after,
.cml-cf9f3b62.cml-border-bottom-1px:after {
  content: "\A0";
  display: block;
  position: absolute;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  z-index: 2;
}

.cml-cf9f3b62.cml-border-1px:after {
  border: 1rpx solid #ccc;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
}

.cml-cf9f3b62.cml-border-top-1px:before {
  border-top: 1rpx solid #ccc;
  left: 0;
  top: 0;
  width: 100%;
}

.cml-cf9f3b62.cml-border-right-1px:after {
  border-right: 1rpx solid #ccc;
  top: 0;
  right: 0;
  height: 100%;
  -webkit-transform-origin: right 0;
  transform-origin: right 0;
}

.cml-cf9f3b62.cml-border-bottom-1px:after {
  border-bottom: 1rpx solid #ccc;
  left: 0;
  bottom: 0;
  width: 100%;
  -webkit-transform-origin: 0 bottom;
  transform-origin: 0 bottom;
}

.cml-cf9f3b62.cml-border-left-1px:before {
  border-left: 1rpx solid #ccc;
  top: 0;
  left: 0;
  height: 100%;
}

.cml-cf9f3b62.cml-border-none:before,
.cml-cf9f3b62.cml-border-none:after {
  display: none;
}

.cml-cf9f3b62.preview-image {
  width: 670rpx;
  height: 300rpx;
}

.cml-cf9f3b62.page-choose-image {
  background-color: #F0F2F7;
}

.cml-cf9f3b62.preview-container {
  width: 750rpx;
  min-height: 350rpx;
  padding: 40rpx 46rpx 70rpx;
  background-color: white;
  margin-top: 20rpx;
}

.cml-cf9f3b62.preview-text {
  font-size: 28rpx;
  color: #192944;
  margin-bottom: 24rpx;
}
